// AVERAGE OF ODD NUMBERS BELOW RANGE
#include <stdio.h>
int main() {
       int num,i,average,add=0;
       printf("Odd numbers within:");
       scanf("%d",&num);
       for(i=1;i<num;i++)
       {
           if(i%2!=0)
           {
               add+=i;
           }
       }
      average=add/num;
      printf("Average of odd numbers below range IS %d ",average);
      return 0;
}
